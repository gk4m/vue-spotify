{
  "name": "vue-spotify",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=10.0.0 <=13.0.0"
  },
  "scripts": {
    "serve": "vue-cli-service serve --https --open",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "iconfont": "gulp iconfont",
    "clean": "rimraf node_modules",
    "deploy": "npm run build && push-dir --dir=dist --branch=gh-pages --allow-unclean --force"
  },
  "dependencies": {
    "axios": "0.19.0",
    "optiscroll": "3.2.1",
    "vue": "2.6.10",
    "vue-awesome": "3.5.4",
    "vue-js-modal": "1.3.31",
    "vue-lazyload": "1.3.2",
    "vue-moment": "4.0.0",
    "vue-router": "3.1.2",
    "vue-slider-component": "2.7.2",
    "vuex": "3.1.1",
    "vuex-persistedstate": "2.5.4"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "3.10.0",
    "@vue/cli-plugin-eslint": "3.10.0",
    "@vue/cli-service": "3.10.0",
    "babel-eslint": "10.0.3",
    "eslint": "6.6.0",
    "eslint-config-prettier": "6.5.0",
    "eslint-loader": "3.0.2",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-vue": "6.0.1",
    "gulp": "4.0.2",
    "gulp-iconfont": "10.0.3",
    "gulp-iconfont-css": "3.0.0",
    "node-sass": "4.13.0",
    "normalize-scss": "7.0.1",
    "prettier": "1.19.1",
    "rimraf": "3.0.0",
    "sass-loader": "7.2.0",
    "vue-template-compiler": "2.6.10"
  },
  "babel": {
    "presets": [
      "@vue/app"
    ]
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "prettier/vue",
      "plugin:prettier/recommended"
    ],
    "rules": {
      "no-unused-vars": 0,
      "no-cond-assign": 0,
      "no-console": 0,
      "no-undef": 0,
      "vue/require-v-for-key": 0,
      "vue/valid-v-for": 0
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
